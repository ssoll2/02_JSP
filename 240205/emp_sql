-- SQL 기록용(복구용)


-- EMP테이블
CREATE TABLE EMP (
    EMPNO INT(4) PRIMARY KEY AUTO_INCREMENT,
    ENAME VARCHAR(10),
    JOB VARCHAR(9),
    MGR INT(4),
    HIREDATE DATE,
    SAL DECIMAL(7, 2),
    COMM DECIMAL(7, 2),
    DEPTNO INT(2)
);


